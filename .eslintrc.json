{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "new-cap": "off",
    "max-len": "off",
    "no-unused-vars": "warn",
    "require-jsdoc": "off"
  }
}